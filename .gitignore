# Ignore directories generated by Composer.
.composer/
.ddev/
/drush/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/web/themes/custom/*/.sass-cache
/web/themes/custom/*/css
/web/themes/custom/*/bootstrap
/web/themes/custom/*/node_modules

# Ignore sensitive information.
/web/sites/*/settings.local.php
/web/sites/default/services.yml

# Ignore migrate yaml files as the dependencies
# only get turned on when they are in use
/config/sync/migrate_plus.*.yml

# Ignore Drupal's file directory.
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by IDEs.
/.idea/
/nbproject/*

# Ignore files generayed by Vagrant.
.vagrant/

# Helps GNU Make track if `bundler install`ed.
.bundled

# Created when documentation is generated.
.apt_updated
.dot_installed
.doxygen_installed
.sphinx_installed
web/docs
venv
latex

# Behat reports.
regression_report_html
regression_report
test_suite/local

